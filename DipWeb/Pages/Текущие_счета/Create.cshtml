            <div class="form-group">
                <label asp-for="ТекущиеСчета.ИдентификаторЮридическогоЛица" class="control-label"></label>
                <input asp-for="ТекущиеСчета.ИдентификаторЮридическогоЛица" class="form-control" />
                <span asp-validation-for="ТекущиеСчета.ИдентификаторЮридическогоЛица" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ТекущиеСчета.ИдентификаторФизическогоЛица" class="control-label"></label>
                <input asp-for="ТекущиеСчета.ИдентификаторФизическогоЛица" class="form-control" />
