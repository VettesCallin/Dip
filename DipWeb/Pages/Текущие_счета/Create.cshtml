            </div>
            <div class="form-group">
                <label asp-for="ТекущиеСчета.ДатаЗакрытия" class="control-label"></label>
                <input asp-for="ТекущиеСчета.ДатаЗакрытия" class="form-control" />
                <span asp-validation-for="ТекущиеСчета.ДатаЗакрытия" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Создать" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-page="Index">Вернуться</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
