                <input asp-for="ТекущиеСчета.ИдентификаторКодаВалюты" class="form-control" />
                <span asp-validation-for="ТекущиеСчета.ИдентификаторКодаВалюты" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ТекущиеСчета.Сумма" class="control-label"></label>
                <input asp-for="ТекущиеСчета.Сумма" class="form-control" />
                <span asp-validation-for="ТекущиеСчета.Сумма" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ТекущиеСчета.ПроцентнаяСтавка" class="control-label"></label>
                <input asp-for="ТекущиеСчета.ПроцентнаяСтавка" class="form-control" />
                <span asp-validation-for="ТекущиеСчета.ПроцентнаяСтавка" class="text-danger"></span>
